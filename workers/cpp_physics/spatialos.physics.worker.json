{
  "build": {
    "tasks_filename": "build.json"
  },
  "bridge": {
    "worker_attribute_set": {
      "attributes": [
        "physics"
      ]
    },
    "component_delivery": {
      "default": "RELIABLE_ORDERED",
      "checkout_all_initially": false,
      "override": {}
    }
  },
  "managed": {
    "linux": {
      "artifact_name": "Physics@Linux.zip",
      "command": "./Physics",
      "arguments": [
        "${IMPROBABLE_RECEPTIONIST_HOST}",
        "${IMPROBABLE_RECEPTIONIST_PORT}",
        "${IMPROBABLE_WORKER_ID}"
      ]
    },
    "windows": {
      "artifact_name": "Physics@Windows.zip",
      "command": "./Physics.exe",
      "arguments": [
        "${IMPROBABLE_RECEPTIONIST_HOST}",
        "${IMPROBABLE_RECEPTIONIST_PORT}",
        "${IMPROBABLE_WORKER_ID}"
      ]
    },
    "macos": {
      "artifact_name": "Physics@Mac.zip",
      "command": "./Physics",
      "arguments": [
        "${IMPROBABLE_RECEPTIONIST_HOST}",
        "${IMPROBABLE_RECEPTIONIST_PORT}",
        "${IMPROBABLE_WORKER_ID}"
      ]
    }
  },
  "external": {
    "local": {
      "run_type": "EXECUTABLE_ZIP",
      "windows": {
        "artifact_name": "Physics@Windows.zip",
        "command": "./Physics.exe",
        "arguments": [
          "localhost",
          "7777",
          "PhysicsLocalWindows"
        ]
      },
      "macos": {
        "artifact_name": "Physics@Mac.zip",
        "command": "./Physics",
        "arguments": [
          "localhost",
          "7777",
          "PhysicsLocalMac"
        ]
      },
      "linux": {
        "artifact_name": "Physics@Linux.zip",
        "command": "./Physics",
        "arguments": [
          "localhost",
          "7777",
          "PhysicsLocalLinux"
        ]
      }
    }
  }
}
