{
  "build": {
    "tasks_filename": "build.json"
  },
  "bridge": {
    "worker_attribute_set": {
      "attributes": [
        "client_direct"
      ]
    },
    "component_delivery": {
      "default": "RELIABLE_ORDERED",
      "checkout_all_initially": false,
      "override": {}
    }
  },
  "external": {
    "local": {
      "run_type": "EXECUTABLE_ZIP",
      "linux": {
        "artifact_name": "ClientDirect@Linux.zip",
        "command": "./ClientDirect",
        "arguments": [
          "localhost",
          "7777",
          "ClientLocalLinux"
        ]
      },
      "windows": {
        "artifact_name": "ClientDirect@Windows.zip",
        "command": "./ClientDirect.exe",
        "arguments": [
          "localhost",
          "7777",
          "ClientLocalWindows"
        ]
      },
      "macos": {
        "artifact_name": "ClientDirect@Mac.zip",
        "command": "./ClientDirect",
        "arguments": [
          "localhost",
          "7777",
          "ClientLocalMac"
        ]
      }
    }
  }
}
